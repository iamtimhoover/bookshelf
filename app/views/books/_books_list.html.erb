<ul>
<% @books.each do |book| %>
	<li>
		<%= link_to book_path(book) do %>
			<p>
				<%= book.title %>
			</p>
		<% end %>
			<p>
				<%= book.description %>
			</p>
			<p>
				<%= book.author %>
			</p>
			<p>
				<%= book.year %>
			</p>
			<p>
				<%= book.rating %>
			</p>
		
	</li>
	<% end %>
</ul>